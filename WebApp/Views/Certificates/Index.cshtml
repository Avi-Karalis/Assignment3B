@model IEnumerable<WebApp.Data.Certificate>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Candidate.FirstName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CertificateTitle.Title)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AssessmentTestCode)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ExaminationDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ScoreReportDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CandidateScore)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MaximumScore)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AssessmentResultLabel)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PercentageScore)
            </th>
            <th></th>
        </tr>
    
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Candidate.FirstName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CertificateTitle.Title)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AssessmentTestCode)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ExaminationDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ScoreReportDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CandidateScore)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MaximumScore)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AssessmentResultLabel)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PercentageScore)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id=item.CertificateId }) |
                @Html.ActionLink("Details", "Details", new { id=item.CertificateId }) |
                @Html.ActionLink("Delete", "Delete", new { id=item.CertificateId })
            </td>
        </tr>
    }
    
    </table>
</body>
</html>
